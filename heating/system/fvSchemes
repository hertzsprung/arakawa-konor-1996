// dummy file to placate paraView
FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

offCentre 0.5;
SIgravityWaves true;

divSchemes {
    default         none;
    div(U,u)        Gauss cubicUpwindCPCFit false;
    div(U,theta)    Gauss cubicUpwindCPCFit false;
}

gradSchemes {
    default         none;
    grad(theta)     Gauss linear;
    grad(Exner)     Gauss linear;
}

laplacianSchemes {
    default         none;
    laplacian(gradPcoeff,Exner) Gauss linear uncorrected;
    laplacian((0.5*(((((G*0.5)*Cp)*deltaT)*rhof)*thetaf)),Exner) Gauss linear uncorrected;
}

ddtSchemes {
    default         none;
    ddt(rho)        Euler;
    ddt(rho,theta)  Euler;
    ddt(Psi,Exner)  Euler;
}

fluxRequired
{
    default         no;
    Exner;
}
